							}
						}
					}
				}
			}
		}
		links existence matches {0..1} cardinality matches {0..*; unordered; unique} matches {
            LINK[at0025] occurrences matches {0..*} matches {  -- Encounter identifier
                meaning existence matches {1..1} matches {
                    DV_TEXT matches {  
                        value matches {"related to"}
                    }
                }
