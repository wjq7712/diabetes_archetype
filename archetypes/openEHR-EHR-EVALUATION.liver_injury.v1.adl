							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0016, 	-- None
									at0017, 	-- NAFLD
									at0018]	-- Alcoholic liver damage flag
								}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Alcoholic liver damage level
