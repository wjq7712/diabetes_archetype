							}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Other liver injury content
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {	-- Other liver injury flag
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
				}
			}
		}
	}

