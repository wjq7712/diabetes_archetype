									at0011, 	-- Hepatitis C
									at0019, 	-- None
									at0020, 	-- Hepatitis A
									at0021]	-- Others
								}
							}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {	-- Drugs liver lesion flag
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {	-- Drugs liver lesion content
						value matches {
