							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0005, 	-- Light
									at0006, 	-- Middle
									at0007]	-- High
								}
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- Virus hepatitis type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
